{% extends "base.twig" %}

{% block title %}Sign In - {{ site_name }}{% endblock %}

{% block body %}
	<login-form
		:allow_registration="{{ settings.allow_registration|json_encode }}"
		:email="{{ email|json_encode }}"
		:remembered="{{ remember|json_encode }}">
	</login-form>
{% endblock %}

{% block scripts %}{% endblock %}
